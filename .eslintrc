{
  "plugins": [
    "prettier",
    "import"
  ],
  "extends": [
    "plugin:prettier/recommended",
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": "off",
    "no-console": 0,
    "class-methods-use-this": "off",
    "comma-dangle": "off",
    "prettier/prettier": "error",
    "object-curly-newline": "off",
    "import/no-cycle": "off",
    "no-param-reassign": [2, { "props": false }]
  }
}
